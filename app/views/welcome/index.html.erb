<div class='container hero-unit'>
<h1>Kay Choi</h1>
<table class='table'>
    <thead>
        <tr>
            <th>Project Name</th>
            <th>Description</th>
            <th>URL</th>
        </tr>
    </thead>

    <tbody>
    <% PROJECTS.each do |project,details| %>
        <tr>
            <td><%= project %></td>
            <td><%= details['description'] %></td>
            <td><%= link_to details['url'], details['url'] %></td>
        </tr>
    <% end %>
    </tbody>
</table>

</div>
